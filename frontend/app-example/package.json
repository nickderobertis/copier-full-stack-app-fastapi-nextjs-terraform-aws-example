{
  "name": "app-example",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "pnpm build:css && pnpm build:next",
    "build:css": "npx tailwindcss -i ./styles/input-globals.css -o ./styles/globals.css",
    "build:next": "next build",
    "build-storybook": "build-storybook -s public",
    "dev": "concurrently npm:dev:*",
    "dev:css": "pnpm build:css --watch",
    "dev:server": "next dev",
    "dev-export": "pnpm export && pnpm start-export",
    "export": "pnpm build && pnpm export:next",
    "export:next": "next export",
    "lint": "concurrently npm:lint:*",
    "lint:types": "tsc",
    "lint:eslint": "next lint",
    "lint-fix": "pnpm lint:eslint --fix",
    "prod": "pnpm build && pnpm start-prod",
    "start-prod": "next start",
    "start-export": "http-server out",
    "storybook": "concurrently npm:dev:css npm:start-storybook",
    "start-storybook": "start-storybook -p 6006 -s ./public",
    "test": "jest",
    "test-watch": "pnpm run test --watch"
  },
  "dependencies": {
    "@hookform/resolvers": "^2.9.5",
    "@sentry/nextjs": "^7.12.1",
    "next": "12.2.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.33.1",
    "react-modal": "^3.15.1",
    "react-social-login-buttons": "^3.6.1",
    "reactive-button": "^1.3.12",
    "swr": "^1.3.0",
    "use-deep-compare": "^1.1.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@babel/preset-env": "^7.1.6",
    "@storybook/addon-actions": "^6.5.9",
    "@storybook/addon-essentials": "^6.5.9",
    "@storybook/addon-interactions": "^6.5.9",
    "@storybook/addon-links": "^6.5.9",
    "@storybook/builder-webpack5": "^6.5.9",
    "@storybook/manager-webpack5": "^6.5.9",
    "@storybook/react": "^6.5.9",
    "@storybook/testing-library": "0.0.13",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@types/jest": "^28.1.4",
    "@types/node": "18.0.3",
    "@types/react": "18.0.15",
    "@types/react-dom": "18.0.6",
    "@types/react-modal": "^3.13.1",
    "@types/testing-library__jest-dom": "^5.14.5",
    "babel-loader": "^8.2.5",
    "concurrently": "^7.2.2",
    "eslint": "8.19.0",
    "eslint-config-next": "12.2.2",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-storybook": "^0.5.13",
    "http-server": "^14.1.1",
    "jest": "^28.1.2",
    "jest-environment-jsdom": "^28.1.2",
    "prettier": "^2.7.1",
    "require-from-string": "^2.0.2",
    "tailwindcss": "^3.1.5",
    "typescript": "4.7.4",
    "webpack": "^5.73.0"
  }
}
